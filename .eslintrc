{
	"env": {
		"es6": true,
		"browser": true,
		"jest": true
	},
	"extends": [
		"airbnb-base",
		"plugin:prettier/recommended",
		"prettier"
		
	],
	"plugins": [
		"babel",
		"prettier"
		
	],
	"rules": {
	"no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
	"no-unused-vars": "off",
	"no-param-reassign": 0,
	"prettier/prettier": "error",
	"max-classes-per-file": "off",
	"quotes": ["error", "double"]

	}

}